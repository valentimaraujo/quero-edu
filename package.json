{
  "name": "quero-edu",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/valentimaraujo/quero-edu.git",
  "author": "Valentim Ara√∫jo <valentim_araujo@yahoo.com.br>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "lint": "eslint ./src __tests__ --ext .ts",
    "lint:fix": "eslint ./src __tests__ --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts __tests__/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts __tests__/**/*.ts",
    "typeorm": "ts-node-dev --project ./tsconfig.json -r ./node_modules/tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -c development -n",
    "migration:run": "yarn typeorm migration:run -c development",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node --project ./tsconfig.json -r ./node_modules/tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -c development seed",
    "seed:runAll": "yarn seed:run -s CreateUsers && yarn seed:run -s CreateUniversities && yarn seed:run -s CreateCampus && yarn seed:run -s CreateCourses && yarn seed:run -s CreateOffers",
    "test": "cross-env NODE_ENV=test jest --runInBand"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.2.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.29",
    "typeorm-seeding": "^1.6.1",
    "xss-clean": "^0.1.1"
  }
}
